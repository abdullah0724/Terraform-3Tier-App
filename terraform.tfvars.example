# Copy this file to terraform.tfvars and update with your values

# AWS Configuration
aws_region = "us-west-2"  # Change to your preferred region

# EC2 Configuration
key_pair_name = "my-app-key"  # CHANGE THIS: Your AWS key pair name
instance_type = "t3.micro"

# Database Configuration
db_password = "YourSecurePassword123!"  # CHANGE THIS: Use a strong password
db_name = "myappdb"
db_username = "admin"

# Project Configuration
project_name = "3tier-app"
environment = "dev"